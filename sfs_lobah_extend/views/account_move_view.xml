<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_form_asset_inherit_lobah" model="ir.ui.view">
        <field name="name">account.move.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@id='other_tab_entry']//group[@id='other_tab_entry_group']//group[@name='misc_group']" position="inside">
                <field name="disable_asset_deletion" invisible="not asset_ids"/>
            </xpath>
        </field>
    </record>
</odoo>
